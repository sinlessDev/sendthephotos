{
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- node --import @swc-node/register/esm-register --watch ./src/main.ts",
    "dev:pg": "docker run --rm --name stp-pg -e POSTGRES_USER=stp -e POSTGRES_PASSWORD=stp -e POSTGRES_HOST_AUTH_METHOD=trust -p 5432:5432 postgres:17.4-alpine",
    "dev:tusd": "docker run --rm --name stp-tusd -p 8080:8080 tusproject/tusd:latest -behind-proxy -hooks-http http://host.docker.internal:3030/tusd-hook"
  },
  "dependencies": {
    "drizzle-orm": "^0.42.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.15.6",
    "qrcode": "^1.5.4",
    "valibot": "^1.0.0",
    "zip-stream": "^7.0.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.41.0",
    "@swc-node/register": "^1.10.10",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.2",
    "@types/pg": "^8.11.14",
    "@types/qrcode": "^1.5.5",
    "@types/zip-stream": "^7.0.0",
    "drizzle-kit": "^0.31.0",
    "typescript": "^5.8.3"
  }
